{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch App",
			"console": "integratedTerminal",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/app/main.py"
		},
		{
			"name": "Launch App (args)",
			"console": "integratedTerminal",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/app/main.py",
			"args": [
				"--settings",
				"${workspaceFolder}/settings.json"
			]
		},
		{
			"name": "Launch Module (args)",
			"console": "integratedTerminal",
			"type": "debugpy",
			"request": "launch",
			"module": "app.main",
			"args": [
				"--settings",
				"${workspaceFolder}/settings.json"
			]
		}
	]
}
